<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Qantas_Hotel_Needs_An_ID</fullName>
    <active>true</active>
    <errorConditionFormula>AND( TEXT(ChangeURL__c) == &apos;Qantas Hotels&apos; , OR(ISBLANK(Hotel_ID__c),ISNULL(Hotel_ID__c)))</errorConditionFormula>
    <errorDisplayField>Hotel_ID__c</errorDisplayField>
    <errorMessage>Hotel ID is required for a Qantas Hotel</errorMessage>
</ValidationRule>
